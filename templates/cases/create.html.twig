{% extends 'base.html.twig' %}

{% block title %}DEICRM - Create Case{% endblock %}

{% block body %}



{% include 'inc/messages/flash.html.twig' %}

{% include 'inc/breadcrumb.html.twig' %}


{% include 'cases/inc/breadcrumb.html.twig' %}


{% if form is defined %}


    {{ form_start(form) }}
    {{ form_row(form.Subject) }}
    <div class="row">
      <div class="col">
        {{ form_row(form.Type) }}
      </div>
     <div class="col">
       {{ form_row(form.Priority) }}
     </div>
   </div>


   <div class="row">
    <div class="col">
      {{ form_row(form.Status) }}
    </div>
    <div class="col">
         {{ form_row(form.AccountName) }}
    </div>
  </div>







   {{ form_row(form.Description) }}

   {{ form_row(form.Resolution) }}

   {{ form_row(form.AssignedTo) }}



    {{ form_widget(form) }}
    {{ form_end(form) }}

{% endif %}






{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('datepicker') }}
    {{ encore_entry_script_tags('date') }}
        {{ encore_entry_script_tags('relatedto') }}


{% endblock %}

{% endblock %}
